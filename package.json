{
  "name": "mvc-boilerplate",
  "version": "1.0.0",
  "description": "Sistema de Gerenciamento de Tarefas com PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "init-db": "node scripts/runSQLScript.js",
    "reset-db": "node scripts/runSQLScript.js --reset",
    "reset-db-full": "node scripts/runSQLScript.js --robust",
    "db-help": "node scripts/runSQLScript.js --help"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-ejs-layouts": "^2.5.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "nodemon": "^3.1.10",
    "supertest": "^6.1.3"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "mvc",
    "tarefas"
  ],
  "author": "Henrique Rodrigues Diniz",
  "license": "ISC"
}
